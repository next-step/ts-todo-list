{
  "compilerOptions": {
    "target": "es2015",
    "module": "CommonJS",
    "allowJs": true,
    "declaration": true,
    "outDir": "./built",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
